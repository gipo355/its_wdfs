{
  "name": "webapitest-swagger",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "targets": {
    "build": {
      "executor": "nx:noop",
      "outputs": ["generated/webapitest-swagger"]
    },
    "codegen": {
      "executor": "@nx-dotnet/core:openapi-codegen",
      "options": {
        "openapiJsonPath": "generated/webapitest-swagger/swagger.json",
        "outputProject": "generated-webapitest-types"
      },
      "dependsOn": ["^swagger"]
    }
  },
  "implicitDependencies": ["Apps.webapitest"]
}
